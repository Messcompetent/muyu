<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="start">


    <com.example.xr.muyudemo.ObservableWebView
        android:id="@+id/shouhome"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <!--<WebView-->
    <!--android:id="@+id/shouhome"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent" />-->

    <include
        layout="@layout/app_bar_classshou"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="160dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url01"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url01" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url02"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url02" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url03"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url03" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url04"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url04" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url05"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url05" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url06"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url06" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url07"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url07" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url08"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url08" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url09"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url09" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url10"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url10" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url11"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url11" />

                <LinearLayout
                    android:layout_width="5dp"
                    android:layout_height="60dp" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/url12"
                    android:layout_width="80dp"
                    android:layout_height="60dp"
                    android:onClick="url12" />
            </LinearLayout>

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_gravity="bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="男" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="装" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="推" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="荐" />
                </LinearLayout>

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/man"
                    android:layout_width="0dp"
                    android:layout_height="80dp"
                    android:layout_weight="0.5"
                    android:onClick="man" />

                <com.example.xr.muyudemo.image.SmartImageView
                    android:id="@+id/woman"
                    android:layout_width="0dp"
                    android:layout_height="80dp"
                    android:layout_weight="0.5"
                    android:onClick="woman" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="0.1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="女" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="装" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="推" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="荐" />
                </LinearLayout>
            </LinearLayout>

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_gravity="bottom" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:onClick="diary"
                    android:text="日记"
                    android:textSize="20sp" />

                <LinearLayout
                    android:layout_width="30dp"
                    android:layout_height="0dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:onClick="guanyyu"
                    android:text="关于我们"
                    android:textSize="20sp" />
            </LinearLayout>
        </LinearLayout>
    </android.support.design.widget.NavigationView>

</android.support.v4.widget.DrawerLayout>
